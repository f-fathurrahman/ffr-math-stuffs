Print[]

(* Create a list *)
emptyList = {}

(* Alternative *)
emptyList = List[]

lst = {}
lst1 = {1.4, 2.9, 3.1}
lst2 = {3.8, 4.3, 5.7}

Print["lst1 = ", lst1]
Print["lst2 = ", lst2]
Print["lst1/3 = ", lst1/3]
Print["lst2/3 = ", lst2/3]

(* Operations on list *)
Print["lst1+lst2 = ", lst1+lst2]
Print["lst1-lst2 = ", lst1-lst2]
Print["lst1*lst2 = ", lst1*lst2]
Print["lst1/lst2 = ", lst1/lst2]

(* Nested list *)
lst3 = {lst1, lst2}
lst4 = {lst2, lst1}

Print["lst3 = ", lst3]
Print["lst4 = ", lst4]

Print["\nTableForm of lst3\n"]
Print[TableForm[lst3]]

Print["\nMatrixForm of lst3\n"]
Print[MatrixForm[lst3]]

Print["\nlst3/lst4\n"]
Print[TableForm[lst3/lst4]]

(* Matrices *)
row1 = {1, 3, 4, 5}
row2 = {3, 4, 5, 7}
row3 = {5, 6, 8, 1}
mat1 = {row1, row2, row3}

Print["\nmat1 = \n"]
Print[MatrixForm[mat1]]


(* Constructing list programmatically *)

(*
Function Table can be used:
Table[expr, {(i), (imin), imax, (di)}]
*)
Print[ Table[2, {3}] ]
Print[ Table[i, {i, 3}] ] (* imax => 3 *)
Print[ Table[i, {i, 3, 10}] ] (* di is default to 1*)
Print[ Table[i, {i, 1, 10, 2}] ]

Print[ Table[3*i, {i, 1, 10, 2}] ]

Exit[]
